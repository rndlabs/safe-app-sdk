{
  "name": "@rndlabs/safe-gateway-typescript-sdk",
  "version": "0.0.2",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "git@github.com:safe-global/safe-gateway-typescript-sdk.git",
  "author": "katspaugh",
  "license": "MIT",
  "dependencies": {
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "@types/jest": "^29.2.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "lint": "tsc && eslint './src/**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "unbuild": "rimraf dist",
    "build": "yarn unbuild && tsc && tsc-alias",
    "prepublishOnly": "yarn lint && yarn test:check && yarn build",
    "prettier": "prettier -w './**/*.ts'",
    "test": "jest --watch --coverage .",
    "test:check": "jest --testPathPattern=tests",
    "test:ci": "jest --ci --coverage --json --watchAll=false --testLocationInResults --runInBand --testPathPattern=tests",
    "format:check": "prettier --check \"*/**/*.{js,json,md,ts}\"",
    "format": "prettier --write \"*/**/*.{js,json,md,ts}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --w ."
    ]
  },
  "type": "module"
}
